<!ENTITY % CountryCode "(en | ja | de | fr | es | it | nl | pt | da | sv | fi | no | is | cs | 
                         hu | pl | sl | sq | tr | el | ru | uk | la | he | zh | th | vi)">
<!ELEMENT WireList (Area)+>
<!ATTLIST WireList
	ver CDATA #IMPLIED
	pubno CDATA #REQUIRED
	xml:lang %CountryCode; #REQUIRED
>
<!ELEMENT Area (Wire)+>
<!ATTLIST Area
	code ID #REQUIRED
>
<!ELEMENT Wire (#PCDATA)>
<!ATTLIST Wire
	code CDATA #REQUIRED
	color CDATA #REQUIRED
	id CDATA #REQUIRED
>

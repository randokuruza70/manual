<!ENTITY % CountryCode "(en | ja | de | fr | es | it | nl | pt | da | sv | fi | no | is | cs | 
                         hu | pl | sl | sq | tr | el | ru | uk | la | he | zh | th | vi)">
<!ENTITY % Number "NMTOKEN">
<!ELEMENT AreaList (Area)+>
<!ATTLIST AreaList
	ver CDATA #IMPLIED
	xml:lang %CountryCode; #REQUIRED
>
<!ELEMENT Area (#PCDATA)>
<!ATTLIST Area
	no %Number; #IMPLIED
	code ID #REQUIRED
>
